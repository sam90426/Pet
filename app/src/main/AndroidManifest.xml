<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sam.pet">

	<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.sam.pet.AccountView.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.sam.pet.UserView.Index" />
        <activity android:name="com.sam.pet.AccountView.Register" />
        <activity android:name="com.sam.pet.AccountView.Find"></activity>
    </application>

</manifest>
